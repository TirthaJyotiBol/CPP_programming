# include <iostream>
using namespace std;

int add(int a,int b){
    int c;
    c= a+b;
    return c;
}

int main()
{
    int a,b,c;
    cout<<"Enter first number "<<endl;
    cin>>a;
    
    cout<<"Enter second number "<<endl;
    cin>>b;
    cout<<"enter third number"<<endl;
    cin>>c;
    
    cout<<"The addition of two numbers is "<<add(a,b)<<endl;
     cout<<"The addition of two numbers is "<<add(a,c);
    
}

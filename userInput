# include <iostream>
using namespace std;

int main(){
    int a,b;
    cout<<"Enter first Number"<<endl;
    cin>>a;
    
    cout<<"Enter second Number"<<endl;
    cin>>b;
    
    cout<<"The sum of the two numbers are : "<<a+b<<endl;
    
    cout<<"The substract of the two numbers are : "<<a-b<<endl;
    
    cout<<"The division of the two numbers are : "<<a/b<<endl;
    
    cout<<"The multiplication of the two numbers are : "<<a*b<<endl;
    
    cout<<"The modulo of the two numbers are : "<<a%b<<endl;
}
